    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-react-parser": "5.0.3",
    "jest-axe": "8.0.0",
    "jest-coverage-badges": "1.1.2",
    "jsdom": "22.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svelte": "4.2.2",
    "typescript": "5.2.2",
    "vitest": "0.34.6",
    "vue": "3.3.7"
