{
  "name": "@avaya/design-portal",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "all": "yarn && yarn clean && yarn style-check && yarn build && yarn test:badges",
    "github": "yarn && yarn clean && yarn lint && yarn build && yarn test",
    "clean": "rm -rf coverage dist",
    "format": "biome format --write ./src",
    "lint": "biome ci ./src",
    "style-check": "biome check --write ./src",
    "build": "astro build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:badges": "yarn test:coverage && jest-coverage-badges --output ./badges",
    "start": "astro dev",
    "preview": "astro preview"
  },
  "devDependencies": {
    "@astrojs/react": "3.6.2",
    "@astrojs/svelte": "5.7.1",
    "@astrojs/vue": "4.5.1",
    "@biomejs/biome": "1.9.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/estree": "1.0.6",
    "@types/jest-axe": "3.5.9",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "2.1.1",
    "@vitest/ui": "2.1.1",
    "astro": "4.15.8",
    "html-react-parser": "5.1.16",
    "jest-axe": "9.0.0",
    "jest-coverage-badges": "1.1.2",
    "jsdom": "25.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "svelte": "4.2.19",
    "typescript": "5.6.2",
    "vitest": "2.1.1",
    "vue": "3.5.8"
  },
  "dependencies": {
    "@astrojs/netlify": "5.5.3",
    "@astrojs/prefetch": "0.4.1",
    "@avaya/neo-react": "1.3.0",
    "@nanostores/react": "0.8.0",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "diffable-html": "5.0.0",
    "fuse.js": "7.0.0",
    "nanostores": "0.11.3",
    "octokit": "4.0.2",
    "prism-react-renderer": "2.4.0",
    "react-element-to-jsx-string": "15.0.0",
    "react-markdown": "9.0.1"
  }
