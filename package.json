    "jest-coverage-badges": "1.1.2",
    "jsdom": "24.0.0",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "svelte": "4.2.17",
