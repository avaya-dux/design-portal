{
  "name": "@avaya/design-portal",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "all": "yarn && yarn clean && yarn format && yarn lint && yarn build && yarn test:badges",
    "github": "yarn && yarn clean && yarn lint && yarn build && yarn test",
    "clean": "rm -rf coverage dist",
    "format": "yarn prettier --write ./src --log-level warn --plugin prettier-plugin-astro",
    "lint": "eslint src",
    "build": "astro build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:badges": "yarn test:coverage && jest-coverage-badges --output ./badges",
    "start": "astro dev",
    "preview": "astro preview"
  },
  "devDependencies": {
    "@astrojs/react": "3.0.4",
