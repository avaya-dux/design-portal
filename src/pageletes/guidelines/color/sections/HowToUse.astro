---
import {
Description,
  PageleteSection,
  SectionHeader,
} from "components";

import { topics } from "../static";
const { id, title } = topics.how;

import { Image } from "@avaya/neo-react";

import image1 from "../temp-imgs/Frame 34421.svg";
---

