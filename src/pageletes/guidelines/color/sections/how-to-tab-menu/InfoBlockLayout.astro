---
import { ImageWithCaption } from "components";

export interface Props {
  graphicIcon?: string;
  graphicAltText?: string;
  header?: string;
  description?: string;
}
const {
  graphicIcon = "no data found",
  graphicAltText = "no data found",
  header = "no data found",
  description = "no data found",
} = Astro.props as Props;
---

<ImageWithCaption
  imgSrc={graphicIcon}
  imgAlt={graphicAltText}
  captionTitle={header}
>
  {description}
</ImageWithCaption>
