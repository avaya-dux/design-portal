---
import SectionLayout from "../../SectionLayout.astro";

import graphic from "../../../../imgs/color-principles-4.svg";

import { Description } from "components";

const altText = "Graphic representing the role colors plays when determining importance";
const type = "Hierarchical";
---

<SectionLayout graphicIcon={graphic.src} graphicAltText={altText} type={type}>
  <Description slot="description">
    Color should indicate which elements are interactive, as well as their level of prominence.
  </Description>
</SectionLayout>
