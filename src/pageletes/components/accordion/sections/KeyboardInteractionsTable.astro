---
import {
  InfoTable,
  InfoTableBody,
  InfoTableCell,
  InfoTableCellHead,
  InfoTableHead,
  InfoTableRow,
} from "components";
---

<InfoTable>
  <InfoTableHead>
    <InfoTableRow>
      <InfoTableCellHead>Keystrokes</InfoTableCellHead>
      <InfoTableCellHead>Interaction</InfoTableCellHead>
    </InfoTableRow>
  </InfoTableHead>

  <InfoTableBody>
    <InfoTableRow>
      <InfoTableCell>Tab</InfoTableCell>
      <InfoTableCell> Moves to the next interactive element. </InfoTableCell>
    </InfoTableRow>

    <InfoTableRow>
      <InfoTableCell>Shift + Tab</InfoTableCell>
      <InfoTableCell>
        Moves to the previous interactive element.
      </InfoTableCell>
    </InfoTableRow>

    <InfoTableRow>
      <InfoTableCell>Enter or Spacebar</InfoTableCell>
      <InfoTableCell>
        Expands or collapses the accordion when it has focus.
      </InfoTableCell>
    </InfoTableRow>
  </InfoTableBody>
</InfoTable>
