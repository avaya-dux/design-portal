---
import {
  InfoTable,
  InfoTableBody,
  InfoTableCell,
  InfoTableCellHead,
  InfoTableHead,
  InfoTableRow,
} from "components";
---

<InfoTable>
  <InfoTableHead>
    <InfoTableRow>
      <InfoTableCellHead>Keystrokes</InfoTableCellHead>
      <InfoTableCellHead>Interaction</InfoTableCellHead>
    </InfoTableRow>
  </InfoTableHead>

  <InfoTableBody>
    <InfoTableRow>
      <InfoTableCell>Tab</InfoTableCell>
      <InfoTableCell>
        Moves into and out of the input field.
      </InfoTableCell>
    </InfoTableRow>

    <InfoTableRow>
      <InfoTableCell>Shift + Tab</InfoTableCell>
      <InfoTableCell>Moves to the previous input field.</InfoTableCell>
    </InfoTableRow>
  </InfoTableBody>
</InfoTable>
