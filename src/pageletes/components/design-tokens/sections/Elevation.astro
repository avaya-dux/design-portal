---
import {
  Description,
  PageleteSection,
  SectionHeader,
  InfoTableBody,
  InfoTableCell,
  InfoTableRow,
} from "components";

import { TypographyTable } from "../components";

import { topics } from "../static";

const { id, title } = topics.elevation;
---

<PageleteSection>
  <SectionHeader id={id}>{title}</SectionHeader>

  <Description>
    Any field can be further enhanced by the use of drop-shadowing and other
    highlighting techniques.
  </Description>

  <TypographyTable
    title=""
    head1="Token Name"
    head2="Example"
    head3="Description"
  >
    <InfoTableBody>
      <InfoTableRow>
        <InfoTableCell />
        <InfoTableCell />
        <InfoTableCell />
      </InfoTableRow>
      <InfoTableRow>
        <InfoTableCell />
        <InfoTableCell />
        <InfoTableCell />
      </InfoTableRow>
    </InfoTableBody>
  </TypographyTable>
</PageleteSection>
