---
import {
  InfoTable,
  InfoTableBody,
  InfoTableCell,
  InfoTableCellHead,
  InfoTableHead,
  InfoTableRow,
} from "components";
---

<InfoTable>
  <InfoTableHead>
    <InfoTableRow>
      <InfoTableCellHead>Keystrokes</InfoTableCellHead>
      <InfoTableCellHead>Interaction</InfoTableCellHead>
    </InfoTableRow>
  </InfoTableHead>

  <InfoTableBody>
    <InfoTableRow>
      <InfoTableCell>Tab</InfoTableCell>
      <InfoTableCell>
        Moves to the element and displays the tooltip if present.
      </InfoTableCell>
    </InfoTableRow>

    <InfoTableRow>
      <InfoTableCell>Esc</InfoTableCell>
      <InfoTableCell>Dismisses the open tooltip.</InfoTableCell>
    </InfoTableRow>
  </InfoTableBody>
</InfoTable>
