---
import { Description, PageleteSection, SectionHeader } from "components/astro";
import { topics } from "../static";

import {
  Brand,
  Placement,
  Profile,
  Search,
} from "./subsections";

const { id, title } = topics.behavior;
---

<PageleteSection>
  <SectionHeader id={id}>
    {title}
  </SectionHeader>

  <Description>
    There are many different behaviors a header can have.
  </Description>

  <Placement />

  <Brand />

  <Profile />

  <Search />

</PageleteSection>
