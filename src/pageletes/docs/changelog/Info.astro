---
import { PageleteSection, TabMenu } from "components";

const tabNames = [
  "Design Updates",
  "CSS",
  "React"
];

import DesignUpdateLogLayout from "./TabSections/components/DesignUpdateLogLayout.astro";
import CSSLogLayout from "./TabSections/components/CSSLogLayout.astro";
import ReactLogLayout from "./TabSections/components/ReactLogLayout.astro";
---
<PageleteSection>
    <TabMenu client:only="react" panels={tabNames}>
      <DesignUpdateLogLayout slot="design updates"/>
      <CSSLogLayout slot="css"/>
      <ReactLogLayout slot="react"/>
    </TabMenu>
</PageleteSection>