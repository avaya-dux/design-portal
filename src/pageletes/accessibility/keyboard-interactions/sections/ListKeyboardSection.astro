---
import {
  HeaderWithLinkButton,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/list";
---

<PageleteSection>
  <HeaderWithLinkButton>
    <SectionHeader id={topics.list.id}>{topics.list.title} </SectionHeader>
    <LinkButton href="/components/list" variant="secondary" size="compact">Visit List Component</LinkButton>
  </HeaderWithLinkButton>
  <KeyboardInteractionsTable />
</PageleteSection>
