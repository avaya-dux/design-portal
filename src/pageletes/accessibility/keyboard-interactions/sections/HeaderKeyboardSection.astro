---
import {
  HeaderWithLinkButton,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/header";
---

<PageleteSection>
  <HeaderWithLinkButton>
    <SectionHeader id={topics.header.id}>{topics.header.title} </SectionHeader>
    <LinkButton href="/components/header" variant="secondary" size="compact">Visit Header Component</LinkButton>
  </HeaderWithLinkButton>
  <KeyboardInteractionsTable />
</PageleteSection>
