---
import {
  RowAlignmentContainer,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/side-navigation";
---

<PageleteSection>
  <RowAlignmentContainer>
    <SectionHeader id={topics.sidenavigation.id}>{topics.sidenavigation.title} </SectionHeader>
    <LinkButton href="/components/side-navigation" variant="secondary" size="compact">Visit Side Navigation Component</LinkButton>
  </RowAlignmentContainer>
  <KeyboardInteractionsTable />
</PageleteSection>
