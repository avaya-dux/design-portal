---
import {
  HeaderWithLinkButton,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/tabs";
---

<PageleteSection>
  <HeaderWithLinkButton>
    <SectionHeader id={topics.tabs.id}>{topics.tabs.title} </SectionHeader>
    <LinkButton href="/components/tabs" variant="secondary" size="compact">Visit Tabs Component</LinkButton>
  </HeaderWithLinkButton>
  <KeyboardInteractionsTable />
</PageleteSection>
