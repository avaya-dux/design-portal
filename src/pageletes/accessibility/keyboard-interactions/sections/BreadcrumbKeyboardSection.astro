---
import {
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/breadcrumb";
---

<PageleteSection>
  <SectionHeader id={topics.breadcrumb.id}>
    {topics.breadcrumb.title}
    <LinkButton slot="right-content" href="/components/breadcrumb" variant="secondary" size="compact">
      Visit Breadcrumb Component
    </LinkButton>
  </SectionHeader>
  <KeyboardInteractionsTable />
</PageleteSection>
