---
import {
  HeaderWithLinkButton,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/breadcrumb";
---

<PageleteSection>
  <HeaderWithLinkButton>
    <SectionHeader id={topics.breadcrumb.id}>{topics.breadcrumb.title} </SectionHeader>
    <LinkButton href="/components/breadcrumb" variant="secondary" size="compact">Visit Breadcrumb Component</LinkButton>
  </HeaderWithLinkButton>
  <KeyboardInteractionsTable />
</PageleteSection>
