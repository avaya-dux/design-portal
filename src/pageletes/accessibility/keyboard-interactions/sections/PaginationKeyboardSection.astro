---
import {
  HeaderWithLinkButton,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "../../../components/pagination/sections";
---

<PageleteSection>
  <HeaderWithLinkButton>
    <SectionHeader id={topics.pagination.id}>{topics.pagination.title} </SectionHeader>
    <LinkButton href="/components/pagination" variant="secondary" size="compact">Visit Pagination Component</LinkButton>
  </HeaderWithLinkButton>
  <KeyboardInteractionsTable />
</PageleteSection>
