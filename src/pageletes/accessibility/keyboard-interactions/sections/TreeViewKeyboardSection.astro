---
import {
  RowAlignmentContainer,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/tree-view";
---

<PageleteSection>
  <RowAlignmentContainer>
    <SectionHeader id={topics.treeview.id}>{topics.treeview.title} </SectionHeader>
    <LinkButton href="/components/tree-view" variant="secondary" size="compact">Visit Tree View Component</LinkButton>
  </RowAlignmentContainer>
  <KeyboardInteractionsTable />
</PageleteSection>
