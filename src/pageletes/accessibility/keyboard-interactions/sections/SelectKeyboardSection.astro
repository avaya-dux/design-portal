---
import {
  RowAlignmentContainer,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/select";
---

<PageleteSection>
  <RowAlignmentContainer>
    <SectionHeader id={topics.select.id}>{topics.select.title} </SectionHeader>
    <LinkButton href="/components/select" variant="secondary" size="compact">Visit Select Component</LinkButton>
  </RowAlignmentContainer>
  <KeyboardInteractionsTable />
</PageleteSection>
