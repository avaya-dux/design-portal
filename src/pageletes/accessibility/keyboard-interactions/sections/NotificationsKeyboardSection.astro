---
import {
  RowAlignmentContainer,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/notifications";
---

<PageleteSection>
