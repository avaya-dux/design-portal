---
import {
  RowAlignmentContainer,
  LinkButton,
  PageleteSection,
  SectionHeader,
} from "components/astro";

import { topics } from "../static"

import { KeyboardInteractionsTable } from "pageletes/components/tooltip";
---

<PageleteSection>
  <RowAlignmentContainer>
    <SectionHeader id={topics.tooltip.id}>{topics.tooltip.title} </SectionHeader>
    <LinkButton href="/components/tooltip" variant="secondary" size="compact">Visit Tooltip Component</LinkButton>
  </RowAlignmentContainer>
  <KeyboardInteractionsTable />
</PageleteSection>
